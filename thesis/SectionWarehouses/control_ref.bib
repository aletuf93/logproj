@article{Malmborg1990,
abstract = {The cube-per-order index (COI) is a commonly used storage assignment policy for allocating inventory in a warehouse. The optimality of COI-based assignment policy for a dual-address order-picking discipline (with storage and retrieval interleaving) has been established. Additional empirical evidence based on recent studies and counterexamples reveal the nonuniqueness of the COI layout and questions the validity of the earlier proof. A completely revised proof of optimality of the COI assignment policy for dual-address systems is presented, taking into account the nonuniqueness of the COI layout. A of the optimality of the COI layout in comparison with the familiar L-shaped and Organ Pipe configurations is provided. ?? 1990.},
author = {Malmborg, Charles J. and Bhaskaran, Krishnakumar},
doi = {10.1016/0307-904X(90)90076-H},
file = {:D$\backslash$:/OneDrive - Alma Mater Studiorum Universit{\`{a}} di Bologna/ACADEMICS/[514]Dottorato/Literature/Mendeley/Malmborg, Bhaskaran - 1990 - A revised proof of optimality for the cube-per-order index rule for stored item location.pdf:pdf},
issn = {0307904X},
journal = {Applied Mathematical Modelling},
keywords = {cube per order index,layout,optimization,order picking,warehousing},
mendeley-groups = {01 WAREHOUSE/03 OperationalStrategy/02{\_}Storage/03{\_}Assignment},
number = {2},
pages = {87--95},
title = {{A revised proof of optimality for the cube-per-order index rule for stored item location}},
volume = {14},
year = {1990}
}

