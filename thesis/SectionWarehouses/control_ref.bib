year = {1983}
}

@article{Chew1999,
abstract = {A travel time model with general item location assignment in a rectangular warehouse system is presented. We give the exact probability mass functions that characterise the tour of an order picker and derive the first and second moments associated with the tour. We apply the model to analysing order batching and storage allocation strategies in an order picking system. The order picking system is modelled as a queueing system with customer batching. The results are compared and validated via simulation. The effects of batching and batch size on the delay time are discussed with consideration to the picking and sorting times for each batch of orders.},
author = {Chew, Ek Peng and Tang, Loon Ching},
