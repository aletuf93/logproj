@article{Malmborg2000,
abstract = {A mathematical model is formulated to capture the impact of item, equipment, storage configuration and operating parameters in less than unit load orderpicking systems. Storage area material flow models based on lot size reorder point policies are integrated with orderpicking travel time models to generate aggregate retrieval area space requirements for randomized storage systems. These results are used to develop cycle time models for single and dual command orderpicking transactions and to evaluate the impact of alternative interleaving disciplines on system performance. The combined model is applied to predict the operating performance of a lot size, reorder point stock management system with respect to item retrieval throughput capacity, physical storage space requirements, inventory service level and system responsiveness. A sample problem is used to illustrate the application of the model as a design tool in measuring the impact of alternative design and operating scenarios on expected system performance.},
author = {Malmborg, Charles J. and Al-Tassan, Khalid},
doi = {10.1016/S0307-904X(99)00009-8},
file = {:D$\backslash$:/OneDrive - Alma Mater Studiorum Universit{\`{a}} di Bologna/ACADEMICS/[514]Dottorato/Literature/Mendeley/Malmborg, Al-Tassan - 2000 - An integrated performance model for orderpicking systems with randomized storage.pdf:pdf},
isbn = {0307-904X},
issn = {0307904X},
journal = {Applied Mathematical Modelling},
keywords = {interleaving,less than unit load,orderpicking,randomized storage,stock management,warehousing},
mendeley-groups = {01 WAREHOUSE/02 WareHouseDesign/03{\_}DepartmentLayout/03 AS/RS {\&} Carousel},
pages = {95--111},
title = {{An integrated performance model for orderpicking systems with randomized storage}},
volume = {24},
year = {2000}
}

@article{Manzini2007,
abstract = {In recent years the increase in time-based competition and the growth of e-commerce have put pressure on managers to reduce the costs of warehouse design and management activities. This paper presents a new integrated approach to support the decision making process in optimising a picker to part, forward-reserve, less than unit load order picking system. An analytical model and a multi-parametric dynamic model to quickly estimate the travelled distance during a picking cycle are introduced. The factorial analysis of several what-if scenarios reveals which factors and combinations of factors are the most critical in affecting the picking system's response.},
